{
  "name": "internship-graphql-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install && yarn gql-codegen:gen",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix ./src --ext .jsx,.ts,.tsx",
    "prettier:format": "prettier --config .prettierrc \"**/*.ts?(x)\" --write",
    "commit": "cz",
    "svg:transform": "node_modules/@svgr/cli/bin/svgr --ignore-existing --no-index --typescript  src/shared/icons/svg",
    "type-check": "tsc --pretty --noEmit",
    "gql-codegen:gen": "graphql-codegen --config gql-codegen.yml -r dotenv/config",
    "gql-codegen:rm": "find . -regex './src/.*__generated__.*' -type d -prune -exec rm -r \"{}\" \\;"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@graphql-codegen/cli": "^3.2.2",
    "@hookform/resolvers": "^3.0.0",
    "@types/node": "18.15.5",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "^10.4.14",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "graphql": "^16.6.0",
    "lodash": "^4.17.21",
    "postcss": "^8.4.21",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-router-dom": "^6.10.0",
    "react-toastify": "^9.1.2",
    "tailwindcss": "^3.2.7",
    "typescript": "5.0.2",
    "validator": "^13.9.0",
    "vite-tsconfig-paths": "^4.0.7",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@graphql-codegen/fragment-matcher": "^4.0.0",
    "@graphql-codegen/introspection": "^3.0.0",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typescript": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^3.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@svgr/cli": "^7.0.0",
    "@svgr/webpack": "^7.0.0",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/validator": "^13.7.13",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "cz-conventional-changelog-for-jira-smart-commits": "^1.0.5",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "typescript": "^4.9.3",
    "validate-branch-name": "^1.3.0",
    "vite": "^4.2.0",
    "yarn": "^1.22.19"
  }
}
